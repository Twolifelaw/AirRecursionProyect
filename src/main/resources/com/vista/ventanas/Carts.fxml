         <children>
            <Label fx:id="lblPais" text="Pais:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="lblCiudad" text="Ciudad:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="lblClima" text="Clima:" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="btnComprar" mnemonicParsing="false" text="Comprar">
               <VBox.margin>
                  <Insets bottom="5.0" left="70.0" />
